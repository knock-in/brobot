{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": ["node_modules", "dist"]
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "systemName"            : "Brobot",
        "footer"                : " ",
        "copyright"             :  "",
        "includeDate"           : "true",
        "navType"               : "vertical",
        "theme"                 : "slate",
        "linenums"              : "true",
        "collapseSymbols"       : "true",
        "inverseNav"            : "true",
        "outputSourceFiles"     : "true" ,
        "outputSourcePath"      : "true",
        "dateFormat"            : "DD.MM.YYYY",
        "syntaxTheme"           : "default",
        "sort"                  : "true"
    }
}